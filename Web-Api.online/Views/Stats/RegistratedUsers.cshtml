@{
    ViewData["Title"] = "RegistratedUsers";
}

<div class="row">

    <table id="RegistratedUsers" class="table table-striped table-centered mb-0">
        <thead>
        <tr class="table-primary">
            <th>№</th>
            <th>Email</th>
            <th>Registration Date</th>
                
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.RegistratedUsers)
        {
            <tr>
                <td>
                    @item.Number
                </td>
                <td>
                    @item.Email
                </td>
                <td>
                    @item.RegistrationDate.ToString("dd-MM-yyyy HH:mm:ss")
                </td>
            </tr>
        }
        </tbody>
    </table>
</div>